{
  "name": "@nimipay/acuity-integration",
  "version": "1.0.0",
  "description": "Nimipay integration for Acuity Scheduling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Nimipay",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nimipay/nimipay-pos-gateway"
  },
  "keywords": [
    "nimipay",
    "acuity",
    "scheduling",
    "payments",
    "integration"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "test": "jest",
    "test:integration": "jest --config jest.config.integration.js",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "npm run build",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepack": "npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "oauth-1.0a": "^2.2.6",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
